pollutantmean <- function(directory, pollutant, id = 1:332){
files <- list.files(directory)
##setwd(file.path(getwd(), directory))
filepath = paste(getwd(),directory, sep="/")
setwd(filepath)
total = 0
noofelements = 0
for (i in id){
inputfile = read.csv( files[[i]])
if ( pollutant == "nitrate"){
data <- na.omit(inputfile$nitrate)
}else {
data <- na.omit(inputfile$sulfate)
}
total = total + sum(data)
nofelements = noofelements + length(data)
}
setwd("..")
total/noofelements
}
